X_train,X_test,y_train,y_test=train_test_split(X,Y,test_size=0.1,random_state=75)
random_forest = RandomForestClassifier(n_estimators = 50, max_depth = 3, random_state = 42)
random_forest.fit(X_train, y_train)

prediction_values_rf = random_forest.predict(X_test)
print(metrics.classification_report(y_test, prediction_values_rf, digits = 2))
print(metrics.confusion_matrix(y_test, prediction_values_rf))
print("")
print("Accuracy: ",metrics.accuracy_score(y_test, prediction_values_rf,))
print("Recall: ",metrics.recall_score(y_test, prediction_values_rf, zero_division=1))
print("Precision: ",metrics.precision_score(y_test, prediction_values_rf, zero_division=1))
